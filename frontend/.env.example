# 工作流名称
NEXT_PUBLIC_WORKFLOW=beanai

# WebSocket 聊天完成 URL
NEXT_PUBLIC_WEBSOCKET_CHAT_COMPLETION_URL=ws://127.0.0.1:8001/websocket

# HTTP 聊天完成 URL
NEXT_PUBLIC_HTTP_CHAT_COMPLETION_URL=http://127.0.0.1:8001/chat/stream

# WebSocket 默认开启
NEXT_PUBLIC_WEB_SOCKET_DEFAULT_ON=false

# 聊天历史默认开启
NEXT_PUBLIC_CHAT_HISTORY_DEFAULT_ON=false

# 右侧菜单打开
NEXT_PUBLIC_RIGHT_MENU_OPEN=false

# 阿里云 OSS 配置（可选，用于图片上传）
REACT_APP_OSS_REGION=oss-cn-beijing
REACT_APP_OSS_ACCESS_KEY_ID=your_access_key_id_here
REACT_APP_OSS_ACCESS_KEY_SECRET=your_access_key_secret_here
REACT_APP_OSS_BUCKET=your_bucket_name_here
